{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "extends": [
      "@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
